        "react-router-dom": "^5.2.0",
        "ts-loader": "^8.0.6",
        "typescript": "^4.0.3"
    },
      "jest": {
        "transform": {
          "^.+\\.(js|jsx|mjs)$": "babel-jest",
          "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "transformIgnorePatterns": [
          "/node_modules/(?!(xxxx.*?\\.js$))"
        ],
        "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "testPathIgnorePatterns": [
          "/node_modules/"
        ],
        "moduleFileExtensions": [
          "ts",
          "tsx",
          "js"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/setupTests.js"
        ]
    }
}
