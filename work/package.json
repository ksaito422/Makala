        "ts-jest": "^26.4.4"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@types/node": "^14.14.6",
        "@types/react": "^16.9.56",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.6",
        "@uiw/react-markdown-preview": "^1.0.9",
        "commander": "^6.2.0",
        "mix-env-file": "^0.1.0",
        "react": "^16.14.0",
